{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "clockifyID": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "hubspotID": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "invoices": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::invoice.invoice",
      "mappedBy": "project"
    },
    "condition": {
      "type": "enumeration",
      "enum": [
        "red",
        "amber",
        "green"
      ],
      "default": "green"
    },
    "stage": {
      "type": "enumeration",
      "enum": [
        "Meeting Scheduled",
        "Bid Preparation",
        "Grant Writing",
        "Submitted to Funder",
        "Not Funded",
        "Awaiting Allocation",
        "Funded & Awaiting Allocated",
        "Allocated",
        "Completed"
      ],
      "required": true
    },
    "costModel": {
      "type": "enumeration",
      "enum": [
        "Facility",
        "Directly Incurred",
        "JobsOC",
        "Voluntary"
      ],
      "required": true,
      "default": "Facility"
    },
    "awardStage": {
      "type": "enumeration",
      "enum": [
        "Pre-Award",
        "Post-Award",
        "Underwrite",
        "Centrally Awarded"
      ],
      "default": "Pre-Award",
      "required": true
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "funder": {
      "type": "string"
    },
    "school": {
      "type": "string"
    },
    "faculty": {
      "type": "enumeration",
      "enum": [
        "Science, Agriculture & Engineering",
        "Medical Sciences",
        "Humanities & Social Sciences",
        "Central"
      ],
      "required": true
    },
    "amount": {
      "type": "decimal",
      "min": 0
    },
    "value": {
      "type": "decimal",
      "min": 0
    },
    "financeContact": {
      "type": "string"
    },
    "account": {
      "type": "string"
    },
    "nuProjects": {
      "type": "string"
    },
    "contacts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::contact.contact",
      "mappedBy": "projects"
    },
    "assignments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::assignment.assignment",
      "mappedBy": "project"
    }
  }
}
